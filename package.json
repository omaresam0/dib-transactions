{
  "name": "dib-transactions",
  "version": "0.0.0",
  "scripts": {
    "build": "env-cmd ng build --configuration production --project elements --output-hashing none --single-bundle",
    "watch": "env-cmd ng build --configuration production --project elements --output-hashing none --single-bundle --watch --plugin @sassoftware/vi-solution-extension-upload/src/upload-bundle.ngx-plugin",
    "watch:debug": "env-cmd ng build --configuration development --project elements --output-hashing none --single-bundle --watch --plugin @sassoftware/vi-solution-extension-upload/src/upload-bundle.ngx-plugin",
    "create:solution-control": "env-cmd ng g @sassoftware/vi-solution-extension-angular-schematics:wc --project components",
    "add-mobile": "ng g @sassoftware/vi-solution-extension-angular-schematics:add-mobile"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/elements": "^18.2.13",
    "@angular/forms": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "@sassoftware/vi-api": "latest",
    "ag-grid-angular": "^33.3.2",
    "ag-grid-community": "^33.3.2",
    "ngx-build-plus": "^18.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.20",
    "@angular/cli": "^18.2.20",
    "@angular/compiler-cli": "^18.2.0",
    "@sassoftware/vi-solution-extension-angular-schematics": "^3.0.0",
    "@sassoftware/vi-solution-extension-upload": "^0.7.0",
    "@types/jasmine": "~5.1.0",
    "env-cmd": "^10.1.0",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^18.2.0",
    "typescript": "~5.5.2"
  }
